    "configurations": [
        {
            "name": "(lldb) Launch bl-tool (debug)",
            "type": "lldb",
            "request": "launch",
            "program": "${env:HOME}/dev/github/swblocks-baselib/bld/d20-a64-clang1205-debug/apps/bl-tool/bl-tool",
