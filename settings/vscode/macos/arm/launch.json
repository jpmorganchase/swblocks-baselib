        {
            "name": "(lldb) Launch bl-tool (release)",
            "type": "lldb",
            "request": "launch",
            "program": "${env:HOME}/dev/github/swblocks-baselib/bld/d20-a64-clang1205-release/apps/bl-tool/bl-tool",
            "args": [ "http", "request", "-s", "-h", "en.wikipedia.org", "--path", "/wiki/Probability_theory" ],
